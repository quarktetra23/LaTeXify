{
  "manifest_version": 2,
  "name": "LaTeXify",
  "version": "1.0",
  "description": "Render LaTeX on web pages",
  "icons": {
    "48": "icons/border-48.png"
  },
  "permissions": [
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content-script.js"]
    }
  ],
  "browser_action": {
    "default_icon": "icons/border-48.png",
    "default_popup": "popup.html"
  }
}
